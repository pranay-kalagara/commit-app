{
  "name": "commit-backend",
  "version": "1.0.0",
  "description": "Commit App - Social Accountability Platform Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/working-server.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "db": "node src/db-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "migrate": "node dist/migrations/migrate.js",
    "seed": "node dist/migrations/seed.js"
  },
  "keywords": [
    "social",
    "accountability",
    "goals",
    "progress-tracking"
  ],
  "author": "Commit App Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^7.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
